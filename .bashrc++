# Remove ' from ls command terminal output
export QUOTING_STYLE=literal

# Alias for list
alias ll="ls -alF"
alias l="ls"

# Alias for grep
alias grep='grep -n --color'

# Alias for piping into clibpard (e.g. 'echo -n testing | clipcopy')
alias clipcopy="xclip -selection clipboard -i"

# Comment this line if powerline is not installed
source ~/.bashrc_powerline
